{
  "name": "vue-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve": "vite",
    "dev": "vite --mode development",
    "pro": "vite --mode production",
    "build": "run-p type-check && vite build",
    "build:dev": "run-p type-check && vite build --mode development",
    "build:pro": "run-p type-check && vite build --mode production",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:lint-staged": "lint-staged",
    "format": "prettier --write src/",
    "prepare": "husky install"
  },
  "dependencies": {
    "dayjs": "^1.11.10",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.18.5",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.18.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.31",
    "postcss-html": "^1.5.0",
    "prettier": "^3.1.0",
    "prettier-eslint": "^16.1.2",
    "rollup-plugin-external-globals": "^0.9.1",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^4.4.0",
    "stylelint-config-recommended-scss": "^13.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "terser": "^5.24.0",
    "typescript": "~5.2.0",
    "vite": "^4.4.11",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.0",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^1.8.19"
  }
}
